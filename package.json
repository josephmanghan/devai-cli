{
  "name": "ollatool",
  "version": "0.1.0",
  "description": "Local-first CLI tool for AI-powered git commit message generation using Ollama",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "npm run format",
    "test": "vitest run",
    "test:unit": "vitest run --dir src",
    "test:integration": "vitest run --dir tests",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.14",
    "debug": "^4.4.3",
    "execa": "^9.6.0",
    "prettier": "^3.0.0",
    "prettier-plugin-embed": "^0.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "keywords": [
    "ai-agents",
    "flash",
    "gems",
    "gemini",
    "prompt-engineering",
    "context-engineering"
  ],
  "author": "Dr Joe (Joseph Manghan)"
}
